algoritmo "While"
var
Contador, C, CQ: Inteiro
inicio
Contador <- 0
Escreva("Quer que conte ate quanto? ")
Leia(C)
Escreva("Quer que conte de quanto em quanto? ")
Leia(CQ)
Enquanto (Contador <= C) faca
         Escreval(Contador)
         Contador <-Contador + CQ
FimEnquanto
Escreva("Contou ",contador/CQ," Vezes.")
fimalgoritmo